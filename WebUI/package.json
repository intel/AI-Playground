{
  "name": "ai-playground",
  "private": true,
  "version": "2.6.1-beta",
  "scripts": {
    "dev": "cross-env VITE_DEBUG_TOOLS=\"true\" VITE_PLATFORM_TITLE=\"for Local® Dev™ Mode\" vite",
    "preview": "vite preview",
    "fetch-build-resources": "node --experimental-strip-types ./build/scripts/fetch-python-package-resources.mts",
    "prepare-python-env": "node --experimental-strip-types ./build/scripts/prepare-python-env.mts",
    "patch-nsis-files": "node --experimental-strip-types ./build/scripts/patch-nsis-template.mts",
    "prepare-build": "npm run fetch-build-resources && npm run prepare-python-env",
    "build": "npm run patch-nsis-files && cross-env-shell VITE_PLATFORM_TITLE=\"for Intel® Arc™\" \"vue-tsc && vite build && electron-builder --config build/build-config.json --win --x64 --publish never\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint:eslint": "eslint . --fix",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "lint:ci": "eslint .",
    "format:ci": "prettier --check ."
  },
  "dependencies": {
    "@ai-sdk/openai-compatible": "^0.2.16",
    "@ai-sdk/vue": "^1.2.12",
    "@heroicons/vue": "^2.2.0",
    "@langchain/community": "^0.3.49",
    "@langchain/core": "^0.3.66",
    "@langchain/openai": "^0.6.2",
    "@radix-icons/vue": "^1.0.0",
    "@tailwindcss/vite": "^4.1.11",
    "@vee-validate/zod": "^4.15.1",
    "@vueuse/core": "^13.6.0",
    "ai": "^4.3.19",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "express": "^4.21.2",
    "extract-zip": "^2.0.1",
    "get-port": "^7.1.0",
    "koffi": "^2.12.2",
    "langchain": "^0.3.30",
    "mammoth": "^1.9.1",
    "marked": "^16.1.1",
    "marked-shiki": "^1.2.1",
    "node-fetch": "^3.3.2",
    "ollama": "^0.5.16",
    "partysocket": "^1.1.4",
    "pdf-parse": "^1.1.1",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.4.1",
    "radix-vue": "^1.9.17",
    "reka-ui": "^2.4.1",
    "remove": "^0.1.5",
    "shiki": "^3.9.2",
    "sudo-prompt": "^9.2.1",
    "tailwind-merge": "^3.3.1",
    "uint8array-extras": "^1.4.1",
    "unplugin-auto-import": "^19.3.0",
    "uuid": "^11.1.0",
    "vee-validate": "^4.15.1",
    "vue": "^3.5.17",
    "word-extractor": "^1.0.4",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.7",
    "@types/exif": "^0.6.5",
    "@types/express": "^5.0.3",
    "@types/minimist": "^1.2.5",
    "@types/node": "^22.16.5",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/devtools": "^8.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "adm-zip": "^0.5.16",
    "electron": "^37.2.3",
    "electron-builder": "^26.0.19",
    "electron-builder-squirrel-windows": "^26.0.19",
    "eslint": "^9.31.0",
    "eslint-plugin-oxlint": "^1.8.0",
    "eslint-plugin-vue": "^10.3.0",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5.8.3",
    "vite": "^7.0.5",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.3"
  },
  "overrides": {
    "ip": "https://registry.npmjs.org/neoip/-/neoip-2.1.0.tgz"
  },
  "debug": {
    "env": {
      "VITE_DEV_SERVER_HOSTNAME": "127.0.0.1",
      "VITE_DEV_SERVER_PORT": 25413,
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:25413",
      "VSCODE_DEBUG": 1
    }
  },
  "main": "dist/main/main.js"
}
