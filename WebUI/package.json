{
  "name": "ai-playground",
  "private": true,
  "version": "3.0.1-alpha",
  "scripts": {
    "dev": "cross-env VITE_DEBUG_TOOLS=\"true\" VITE_PLATFORM_TITLE=\"from Intel®\" vite",
    "preview": "vite preview",
    "fetch-build-resources": "node --experimental-strip-types ./build/scripts/fetch-python-package-resources.mts",
    "prepare-python-env": "node --experimental-strip-types ./build/scripts/prepare-python-env.mts",
    "patch-nsis-files": "node --experimental-strip-types ./build/scripts/patch-nsis-template.mts",
    "prepare-build": "npm run fetch-build-resources && npm run prepare-python-env",
    "build": "npm run patch-nsis-files && cross-env-shell VITE_PLATFORM_TITLE=\"from Intel®\" \"vue-tsc && vite build && electron-builder --config build/build-config.json --win --x64 --publish never\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint:eslint": "eslint . --fix",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "lint:ci": "eslint .",
    "format:ci": "prettier --check ."
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.88",
    "@ai-sdk/openai-compatible": "^1.0.29",
    "@ai-sdk/vue": "^2.0.115",
    "@google/model-viewer": "^4.1.0",
    "@heroicons/vue": "^2.2.0",
    "@langchain/community": "^1.1.14",
    "@langchain/core": "^0.3.79",
    "@langchain/openai": "^0.6.16",
    "@radix-icons/vue": "^1.0.0",
    "@tailwindcss/vite": "^4.1.18",
    "@vee-validate/zod": "^4.15.1",
    "@vueuse/core": "^13.9.0",
    "ai": "^5.0.115",
    "autoprefixer": "^10.4.23",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "dompurify": "^3.3.1",
    "extract-zip": "^2.0.1",
    "get-port": "^7.1.0",
    "koffi": "^2.14.1",
    "langchain": "^0.3.36",
    "lucide-vue-next": "^0.562.0",
    "mammoth": "^1.11.0",
    "marked": "^16.4.2",
    "marked-shiki": "^1.2.1",
    "ollama": "^0.6.3",
    "partysocket": "^1.1.6",
    "pdf-parse": "^1.1.1",
    "pinia": "^3.0.4",
    "pinia-plugin-persistedstate": "^4.7.1",
    "radix-vue": "^1.9.17",
    "reka-ui": "^2.6.1",
    "remove": "^0.1.5",
    "shiki": "^3.20.0",
    "sudo-prompt": "^9.2.1",
    "tailwind-merge": "^3.4.0",
    "three": "^0.182.0",
    "uint8array-extras": "^1.5.0",
    "unplugin-auto-import": "^20.3.0",
    "vee-validate": "^4.15.1",
    "vue": "^3.5.26",
    "word-extractor": "^1.0.4",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.7",
    "@types/dompurify": "^3.0.5",
    "@types/exif": "^0.6.5",
    "@types/minimist": "^1.2.5",
    "@types/node": "^22.19.3",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vue/devtools": "^8.0.5",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "adm-zip": "^0.5.16",
    "electron": "^40.0.0",
    "electron-builder": "^26.0.20",
    "electron-builder-squirrel-windows": "^26.0.20",
    "eslint": "^9.36.0",
    "eslint-plugin-oxlint": "^1.33.0",
    "eslint-plugin-vue": "^10.6.2",
    "prettier": "^3.7.4",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.1.8"
  },
  "overrides": {
    "ip": "https://registry.npmjs.org/neoip/-/neoip-2.1.0.tgz",
    "three": "^0.182.0",
    "tar": "^7.5.3"
  },
  "debug": {
    "env": {
      "VITE_DEV_SERVER_HOSTNAME": "127.0.0.1",
      "VITE_DEV_SERVER_PORT": 25413,
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:25413",
      "VSCODE_DEBUG": 1
    }
  },
  "main": "dist/main/main.js"
}
